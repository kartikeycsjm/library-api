{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb-uri"
    }
  }
  